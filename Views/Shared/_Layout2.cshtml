<!DOCTYPE html>
<html>
<head>

    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <meta name="viewport" content="width=device-width" />

</head>
<body>
    <div class="row">
        <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2 " style="height:950px;border:1px solid gray;background-color:dimgrey ">
            <ul class="list-group">
                <li class="list-group-item">
                    @Html.ActionLink("Add Category", "AddCategory")
                </li>
                <li class="list-group-item">
                    @Html.ActionLink("Add Question", "AddQuestion")
                </li>
                <li class="list-group-item">
                    @Html.ActionLink("Report", "Report")
                </li>

                <li class="list-group-item">
                    @Html.ActionLink("LogOut", "LogOut")
                </li>
            </ul>
        </div>
        <div class="col-lg-10 col-md-10 col-sm-10 col-xs-10 " style="height:950px;border:1px solid gray">

            @RenderBody()
        </div>
    </div>
 
</body>
</html>
